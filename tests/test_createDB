from src.soilpulse import db_access

with open("../databases/soilpulse.sql", "r") as file:
    sql = file.read()

db = db_access.DBconnector
cnx = db.db_connection
cursor = cnx.cursor()

cursor.execute(sql)
cursor.fetchall()

